" gvimの設定

set showtabline=2
set transparency=0
set imdisable
set guioptions-=T
set antialias
set tabstop=4
set number
set nobackup
set visualbell t_vb=
set nowrapscan
set columns=100
set lines=48
"set guifont=Monaco:h16
"set guifont=Droid\ Sans\ Mono\ for\ Powerline:h17
set guifont=Monoid\ Regular:h18

"カラースキームの設定
"colorscheme jellybeans
colorscheme molokai
"colorscheme railscasts
"colorscheme hybrid
